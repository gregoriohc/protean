{
    "name": "gregoriohc/protean",
    "type": "library",
    "description": "A framework agnostic, multi-gateway multi-service processing base library for PHP 7.0+",
    "keywords": [
        "gregoriohc",
        "protean"
    ],
    "homepage": "https://github.com/gregoriohc/protean",
    "license": "MIT",
    "authors": [
        {
            "name": "Gregorio Hern√°ndez Caso",
            "email": "gregoriohc@gmail.com",
            "homepage": "https://github.com/gregoriohc",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~5.6|~7.0",
        "illuminate/support": "^5.0,<5.7",
        "illuminate/validation": "^5.0,<5.7"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=4.0",
        "squizlabs/php_codesniffer": "^2.3",
        "mockery/mockery": "~0.9|~1.0",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Gregoriohc\\Protean\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gregoriohc\\Protean\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    }
}
